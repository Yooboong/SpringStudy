{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Email</th>
        <th scope="col">Password</th>
    </tr>
    </thead>
    <tbody>

    <!--  {{#attributeName}} {{/attributeName}} 으로 해당하는 객체를 사용할 범위를 지정 -->

    {{#member}}
        <tr>
            <th>{{id}}</th>
            <td>{{email}}</td>
            <td>{{password}}</td>
        </tr>
    {{/member}}

    </tbody>
</table>

<a href="/members/{{member.id}}/edit">수정하기</a>

<a href="/members">Go to member list</a>

{{>layouts/footer}}