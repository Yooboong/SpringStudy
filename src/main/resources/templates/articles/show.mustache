{{>layouts/header}}

<table class="table">
    <thead>
    <tr>
        <th scope="col">Id</th>
        <th scope="col">Title</th>
        <th scope="col">Content</th>
    </tr>
    </thead>
    <tbody>

    <!--  {{#attributeName}} {{/attributeName}} 으로 해당하는 객체를 사용할 범위를 지정 -->

    {{#article}}
        <tr>
            <th>{{id}}</th>
            <td>{{title}}</td>
            <td>{{content}}</td>
        </tr>
    {{/article}}

    </tbody>
</table>

{{>layouts/footer}}